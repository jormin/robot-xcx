/**index.wxss**/
page{
  padding: 0x;
  margin: 0;
  background: -moz-linear-gradient(-45deg, #183850 0, #183850 25%, #192c46 50%, #22254c 75%, #22254c 100%);
  background: -webkit-linear-gradient(-45deg, #183850 0, #183850 25%, #192c46 50%, #22254c 75%, #22254c 100%);
  background-repeat: no-repeat;
  background-attachment: fixed;
}
.chat-thread {
  width: 100%;
  margin: 0 auto 0 auto;
  padding: 0 0rpx 0 0;
  list-style: none;
  overflow-y: auto;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
}

.chat-thread .message {
  position: relative;
  clear: both;
  display: inline-block;
  padding: 20rpx 40rpx;
  margin: 0 0 30rpx 0;
  font: 32rpx/40rpx 'Noto Sans', sans-serif;
  border-radius: 20rpx;
  background-color: rgba(25, 147, 147, 0.2);
  text-align: left;
}
.chat-thread .message::before {
  position: absolute;
  top: 0;
  width: 80rpx;
  height: 80rpx;
  border-radius: 100rpx;
  content: '';
}
.chat-thread .message::after {
  position: absolute;
  top: 20rpx;
  content: '';
  width: 0;
  height: 0;
  border-top: 30rpx solid rgba(25, 147, 147, 0.2);
}
.chat-thread .message.user {
  animation: show-chat-odd 0.15s 1 ease-in;
  -moz-animation: show-chat-odd 0.15s 1 ease-in;
  -webkit-animation: show-chat-odd 0.15s 1 ease-in;
  float: right;
  margin-right: 120rpx;
  color: #0AD5C1;
}

.chat-thread .message.user:before {
  right: -120rpx;
  background-image: url(data:image/jpeg;base64,);
  background-size: cover;
}

.chat-thread .message.user:after {
  border-right: 30rpx solid transparent;
  right: -30rpx;
}

.chat-thread .message.robot {
  animation: show-chat-even 0.15s 1 ease-in;
  -moz-animation: show-chat-even 0.15s 1 ease-in;
  -webkit-animation: show-chat-even 0.15s 1 ease-in;
  float: left;
  margin-left: 120rpx;
  color: #0EC879;
}

.chat-thread .message.robot:before {
  left: -120rpx;
  background-image: url(data:image/jpeg;base64,);
  background-size: cover;
}

.chat-thread .message.robot:after {
  border-left: 30rpx solid transparent;
  left: -30rpx;
}

.wrap-record {
  position: fixed;
  bottom: 50rpx;
  width: 100%;
}
.btn-record{
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx; 
  line-height: 120rpx;
  font-size: 60rpx;
  background-color: rgba(25, 147, 147, 0.2);
  color:#0AD5C1;
}